{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "urn:otherError#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "message": {
      "type": "string"
    },
    "details": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "reason": {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        }
      }
    }
  },
  "required": [
    "message"
  ]
}